{
  "7277004771816190301": {
    "build": {
      "command": "next build",
      "options": {
        "cwd": "apps/web",
        "tty": false
      },
      "dependsOn": [
        "^build"
      ],
      "cache": true,
      "inputs": [
        "default",
        "^default",
        {
          "externalDependencies": [
            "next"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}/apps/web/.next/!(cache)/**/*",
        "{workspaceRoot}/apps/web/.next/!(cache)"
      ]
    },
    "dev": {
      "continuous": true,
      "command": "next dev",
      "options": {
        "cwd": "apps/web"
      }
    },
    "start": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/web"
      },
      "dependsOn": [
        "build"
      ]
    },
    "serve-static": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/web"
      },
      "dependsOn": [
        "build"
      ]
    },
    "build-deps": {
      "dependsOn": [
        "^build"
      ]
    },
    "watch-deps": {
      "continuous": true,
      "dependsOn": [
        "build-deps"
      ],
      "command": "npx nx watch --projects web --includeDependentProjects -- npx nx build-deps web"
    }
  }
}